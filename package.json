    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
