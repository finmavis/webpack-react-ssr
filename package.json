    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.0",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.1",
    "prettier": "^2.7.0",
    "prop-types": "^15.8.1",
