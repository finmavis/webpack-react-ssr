    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.3.0",
